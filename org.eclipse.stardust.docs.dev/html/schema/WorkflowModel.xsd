            </xs:element>
            <xs:element ref="linkType" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>
                     The list of user defined link types registered for use with this
                     model.
                  </xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="view" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>
