   <xs:element name="model">
      <xs:annotation>
         <xs:documentation>
            The model declaration defines the root element of a model file.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
