<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns="http://www.carnot.ag/workflowmodel/3.1"
   targetNamespace="http://www.carnot.ag/workflowmodel/3.1"
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xmlns:xpdl="http://www.wfmc.org/2008/XPDL2.1"
   elementFormDefault="qualified">

   <xs:import namespace="http://www.wfmc.org/2008/XPDL2.1" schemaLocation="../xpdl/xpdl.xsd" />

   <xs:group name="grDescrAttr">
      <xs:sequence>
         <xs:element ref="description" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>An optional description of the model element.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="attribute" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>The list of extension attributes.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:group>
   <xs:group name="grAttrDescr">
      <xs:sequence>
         <xs:element ref="attribute" minOccurs="1" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>The list of extension attributes.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="description" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>An optional description of the model element.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:group>
   <xs:simpleType name="connectionRoutingType">
      <xs:annotation>
         <xs:documentation>
            Available routing algorithms for connection symbols.
         </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:NCName">
         <xs:enumeration value="Default"/>
         <xs:enumeration value="ShortestPath"/>
         <xs:enumeration value="Manhattan"/>
         <xs:enumeration value="Explicit"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="flowControlType">
      <xs:restriction base="xs:NCName">
         <xs:enumeration value="none"/>
         <xs:enumeration value="join"/>
         <xs:enumeration value="split"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="joinSplitType">
      <xs:restriction base="xs:NCName">
         <xs:enumeration value="None"/>
         <xs:enumeration value="XOR"/>
         <xs:enumeration value="AND"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="swimlaneOrientationType">
      <xs:restriction base="xs:NCName">
         <xs:enumeration value="Vertical"/>
         <xs:enumeration value="Horizontal"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="accessPoint">
      <xs:annotation>
         <xs:documentation>
            The accessPoint declaration defines an access point for the following
            elements: application, context und trigger.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the access point.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the access point.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human readable name of the access point.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="direction" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The direction of the access point. Valid values are &quot;IN&quot;,
                  &quot;OUT&quot; or &quot;INOUT&quot;
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The model id of one of the previously defined dataType elements.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="activity">
      <xs:annotation>
         <xs:documentation>
            The activity declaration defines an activity in the workflow process
            definition.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
            <xs:element ref="dataMapping" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>The list of data mappings.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="eventHandler" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>
                     The list of event handlers of the activity definition.
                  </xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="xpdl:Loop" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>
                     The loop characteristics of the activity.
                  </xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the workflow process definition.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the activity definition.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the activity definition.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="split" type="joinSplitType">
            <xs:annotation>
               <xs:documentation>
                  The type of activity split. Valid values are: &quot;None&quot;,
                  &quot;XOR&quot; or &quot;AND&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="join" type="joinSplitType">
            <xs:annotation>
               <xs:documentation>
                  The type of activity join. Valid values are: &quot;None&quot;,
                  &quot;XOR&quot; or &quot;AND&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="implementation" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  Description of how the activity is implemented. Valid values are
                  &quot;Application&quot;, &quot;Subprocess&quot;, &quot;Route&quot; or
                  &quot;Manual&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="loopType" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  Description of whether the activity shall be executed in a loop. Valid
                  values are &quot;NO LOOP&quot;, &quot;WHILE&quot; or &quot;REPEAT&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="loopCondition" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  Logical condition for the loop when attribute loopType is
                  &quot;WHILE&quot; or &quot;REPEAT&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="performer" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The model id of a participant (role, organization or conditional
                  performer) who is assigned to the activity.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="application" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The model id of the application to execute when the attribute
                  &quot;implementation&quot; is set to &quot;Application&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="implementationProcess" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The model id of the subprocess implementing the activity when the
                  attribute &quot;implementation&quot; is set to &quot;Subprocess&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="subProcessMode">
            <xs:annotation>
               <xs:documentation>
                  The execution mode of the subprocess. &quot;sync_shared&quot; executes
                  the process synchronously (the default mode).
                  &quot;asynch_separate&quot; just triggers a fully independent instance
                  of the implementing process. This attribute is valid only when the
                  attribute &quot;implementation&quot; is set to &quot;Subprocess&quot;.
               </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:NMTOKEN">
                  <xs:enumeration value="sync_shared" />
                  <xs:enumeration value="sync_separate" />
                  <xs:enumeration value="async_separate" />
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="allowsAbortByPerformer" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  Determines whether the activity is allowed to be aborted.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="hibernateOnCreation" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  Determines whether an activity instance is hibernated immediately after
                  beeing created.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="application">
      <xs:annotation>
         <xs:documentation>
            The application declaration defines an application for a workflow activity in
            a workflow process.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
            <xs:element ref="context" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>The list of application contexts.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="accessPoint" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>The list of access points.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the application definition.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the application definition.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the application definition.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The type of the application as java class name. Allowed are
                  subinterfaces of ag.carnot.workflow.spi.ApplicationType
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="interactive" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether this application is interactive or not.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="applicationContextType">
      <xs:annotation>
         <xs:documentation>
            The applicationContextType declaration defines an application context type for
            a workflow activity in a workflow process.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the application contect type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the application context type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the application context type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="predefined" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether the application context type element is
                  predefined or created by modeling.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="hasApplicationPath" type="xs:boolean" />
         <xs:attribute name="hasMappingId" type="xs:boolean" />
         <xs:attribute name="panelClass" type="xs:string" />
         <xs:attribute name="accessPointProviderClass" type="xs:string" />
         <xs:attribute name="validatorClass" type="xs:string" />
      </xs:complexType>
   </xs:element>
   <xs:element name="applicationType">
      <xs:annotation>
         <xs:documentation>
            The applicationType declaration defines an application type for a workflow
            activity in a workflow process.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the application type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the application type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the application type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="predefined" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether the application type element is
                  predefined or created by modeling.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="synchronous" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether the application type element describes
                  a synchronously executed application or not.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="panelClass" type="xs:string" />
         <xs:attribute name="accessPointProviderClass" type="xs:string" />
         <xs:attribute name="validatorClass" type="xs:string" />
         <xs:attribute name="instanceClass" type="xs:string" />
      </xs:complexType>
   </xs:element>
   <xs:element name="attribute">
      <xs:annotation>
         <xs:documentation>
            Defines the syntax for extension attributes. Basically being tagged values,
            attributes allow a nondisruptive evolution of the model file format as well as
            Carnot and third party vendor extensions.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded" />
         </xs:choice>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The name of this attributes. It is recommended to use names with a
                  structure like
                  <code>vendor:component:sub-component:..:property</code>
                  , i.e.
                  <code>carnot:defdesk:icon</code>
                  vs.
                  <code>carnot:engine:defaultValue</code>
                  .
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>
                  The optional type of this attribute. Will be used as hint while
                  attribute value evaluation, if existent.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="value" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>
                  The value of this attribute, if it can be represented as a single line
                  string. Alterntively arbitrary complex child element nodes may be used
                  to represent more complex values.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="bindAction">
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the bind action.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the bind action.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the bind action.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The type of the bind action. Valid values are the model id's of
                  previously defined eventActionType elements whose
                  unsupportedContexts-attribute does not contain "bind".
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="conditionalPerformer">
      <xs:annotation>
         <xs:documentation>
            The conditionalPerformer declaration defines a perfomer under certain
            circumstances.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the conditional performer.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the conditional performer.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the conditional performer.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="is_user" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether this conditional performer is a user or
                  not.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="data" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The model id of the data which determines the runtime identity of the
                  performer.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="dataPath" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The data path the data which determines the runtime identity of the
                  performer. This must evaluate to String (id) or long (oid).
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="context">
      <xs:annotation>
         <xs:documentation>
            The context declaration defines a context for interactive applications where
            context related attributes and access points can be specified.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
            <xs:element ref="accessPoint" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>The list of access points.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the context.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The type of the context for interactive applications. Valid values are
                  the following model id's of previously defined applicationContextType
                  elements: &quot;jfc&quot; | &quot;jsp&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="data">
      <xs:annotation>
         <xs:documentation>
            The data declaration defines a data element for the workflow model.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>A 32-bit number assigned to the data.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the data element.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the data element.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  Abstract description of the data type of the data element (e.g.
                  primitive, entity). Depending on that type class names are given as
                  attribute elements.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="predefined" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether the data element is predefined or
                  created by modeling.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="dataMapping">
      <xs:annotation>
         <xs:documentation>
            The dataMapping declaration defines a mapping between data and activity.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:attribute name="oid" type="xs:long" use="optional">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the data mapping.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the data mapping.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the data mapping.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="data" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The unique model id of the corresponding data element.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="direction" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The direction of the mapping. Valid values are &quot;IN&quot; |
                  &quot;OUT&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="context" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  If the activity is an interactive activity then the context specifies
                  for which type of interactive activity this data mapping is used. Valid
                  values are the following model id's of previously defined
                  applicationContextType elements: &quot;jfc&quot; | &quot;jsp&quot;
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="dataPath" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The method of the data object to retrieve or put the data. Can be left
                  empty, if the whole object is used.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="applicationAccessPoint" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The name of the access point for the application in scope of the
                  activity.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="applicationPath" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The method of the application to retrieve or put the data to/from the
                  application.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="dataPath">
      <xs:annotation>
         <xs:documentation>
            The dataPath declaration defines a data path (data element with a chain of
            accessing methods) for a process definition.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence minOccurs="0">
            <xs:choice minOccurs="0" maxOccurs="1">
               <xs:group ref="grDescrAttr" />
               <xs:group ref="grAttrDescr" />
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the data path.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the data path.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the data path.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="direction" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The direction of the data path. Valid values are &quot;IN&quot; |
                  &quot;OUT&quot;.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="descriptor" type="xs:boolean" />
         <xs:attribute name="data" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The unique model id of the corresponding data element.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="dataPath" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The method of the data object to retrieve or put the data. Can be left
                  empty, if the whole object is used.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="dataType">
      <xs:annotation>
         <xs:documentation>
            The dataType declaration defines a data type for data elements.
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:choice minOccurs="0" maxOccurs="1">
            <xs:group ref="grDescrAttr" />
            <xs:group ref="grAttrDescr" />
         </xs:choice>
         <xs:attribute name="oid" type="xs:long" use="required">
            <xs:annotation>
               <xs:documentation>
                  A 32-bit number assigned to the data type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the data type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  The human friendly name of the data type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="predefined" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  A boolean that indicates whether the data type is predefined or created
                  by modeling.
