            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:string" use="required">
            <xs:annotation>
               <xs:documentation>
                  The human usable identifier of the link type.
               </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string" use="required">
